  <% content_for :title do %>Sonnen Battery<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="charge" data-view="Meter" data-height="150" data-width="150" data-title="Sonnen Carica" data-min="0" data-max="100" data-suffix="%"></div>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="consumption-chart" data-view="Graph" data-title="Consumo" data-moreinfo="Watts" class="mod-red"></div>
    </li>
    
    <li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="production-chart" data-view="Graph" data-title="Produzione" data-moreinfo="Watts" class="mod-green"></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="ecoflow-charge" data-view="Meter" data-height="150" data-width="150" data-title="Ecoflow Carica" data-min="0" data-max="100" data-suffix="%"></div>
    </li>

    <li data-row="2" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="ecoflow-power-out" data-view="Graph" data-title="Ecoflow Consumo" data-moreinfo="Watts" class="mod-red"></div>
    </li>

    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="ecoflow-power-in" data-view="Graph" data-title="Ecoflow Produzione" data-moreinfo="Watts" class="mod-green"></div>
    </li>

    <li data-row="3" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="battery-power-chart" data-view="Graph" data-title="Sonnen Potenza" data-moreinfo="Watts"></div>
    </li>
    
    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="grid-chart" data-view="Graph" data-title="Enel immissione" data-moreinfo="Watts"></div>
    </li>

  </ul>
</div>
